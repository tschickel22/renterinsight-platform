{
  "$schema": "https://unpkg.com/knip@latest/schema.json",
  "entry": ["src/main.tsx", "src/App.tsx"],
  "project": ["src/**/*.{ts,tsx}"],
  "ignoreDependencies": [
    "tailwindcss-animate",
    "jspdf-autotable"
  ],
  "ignoreExportsUsedInFile": true,
  "rules": {
    "binaries": "error",
    "dependencies": "error",
    "devDependencies": "warn",
    "exports": "error",
    "files": "error",
    "nsExports": "error",
    "nsTypes": "error",
    "types": "error"
  }
}